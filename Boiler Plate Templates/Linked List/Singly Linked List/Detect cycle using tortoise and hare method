public class Solution {
    public boolean hasCycle(ListNode head) {
        ListNode tortoise = head;
        ListNode hare= head;
        while(!(hare==null || hare.next==null)){
            hare = hare.next.next;
            tortoise=tortoise.next;
            if(hare==tortoise) return true;
        }
        return false;
    }
}
