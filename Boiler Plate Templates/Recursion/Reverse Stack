class Solution {
    public static void reverseStack(Stack<Integer> st) {
        // code here
        if(st.isEmpty()) return ;
        int top = st.pop();
        reverseStack(st);
        insertBottom(st,top);
    }
    public static void insertBottom(Stack<Integer> st,int x){
        if(st.isEmpty()){
            st.push(x);
            return;
        }
        int top = st.pop();
        insertBottom(st,x);
        st.push(top);
    }
}
